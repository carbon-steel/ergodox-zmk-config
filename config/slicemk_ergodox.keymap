			label = "AUTO_SHIFT";
			#binding-cells = <2>;
			tapping_term_ms = <175>;
			quick_tap_ms = <0>;
			flavor = "tap-preferred";
			bindings = <&kp>, <&kp>;
		};
	        tgl: toggle {
	            compatible = "zmk,behavior-tap-dance";
	            label = "TOGGLE";
	            tapping-term-ms = <200>;
		    #binding-cells = <0>;
	            bindings = <&none>, <&none>, <&tog LAYER_OFF>;
	        };
